  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1",
    "swr": "^0.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "3.2.1",
    "@types/node": "14.0.6",
    "husky": "^4.2.5",
    "jsdom": "16.2.2",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "stylelint": "13.5.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.9.3",
    "xo": "^0.30.0"
