  "version": "1.0.0",
  "description": "Dicionario informal com dialetos de cada regiao brasileira.",
  "keywords": [
    "culture",
    "dictionary",
    "Brazilian dialects",
    "dialetos brasileiros",
    "cultura brasileira",
    "dicionario informal",
    "riqueza linguistica"
  ],
  "scripts": {
    "dev": "next -p 4001",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "xo",
    "test": "jest"
  },
  "xo": {
